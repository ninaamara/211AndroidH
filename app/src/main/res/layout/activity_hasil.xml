<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp"
    tools:context=".HasilActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nama"
        />
    <TextView
        android:id="@+id/namaTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nina Amara"
        android:textSize="24sp"
        android:layout_marginBottom="8dp"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Email"
        />
    <TextView
        android:id="@+id/emailTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ninaamara419@gmail.com"
        android:textSize="24sp"
        android:layout_marginBottom="8dp"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Handphone"
        />
    <TextView
        android:id="@+id/handphoneTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="+621257385676"
        android:textSize="24sp"
        android:layout_marginBottom="8dp"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tanggal Lahir"
        />
    <TextView
        android:id="@+id/tanggallahirTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="22/03/2001"
        android:textSize="24sp"
        android:layout_marginBottom="8dp"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Jarak"
        />
    <TextView
        android:id="@+id/jarakTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="22"
        android:textSize="24sp"
        android:layout_marginBottom="8dp"
        />


</LinearLayout>